{
  "name": "earningsvibe",
  "version": "1.0.0",
  "main": "build/backend/index.js",
  "repository": "https://github.com/oze4/earningsvibe.git",
  "author": "Matt Oestreich <matthewpoestreich@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start:front": "yarn --cwd frontend start",
    "build:front": "yarn --cwd frontend build",
    "install:front": "yarn --cwd frontend install",
    "start:back": "nodemon --watch 'backend' --exec 'ts-node' --project backend/tsconfig.json --log-error backend/index.ts",
    "build:back": "tsc --project backend/tsconfig.json",
    "build": "yarn build:front && yarn build:back",
    "start:dev": "yarn build && node build/backend/index.js",
    "start": "node build/backend/index.js",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "heroku-postbuild": "yarn install:front && yarn build"
  },
  "dependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.35",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "got": "^11.8.2",
    "helmet": "^4.4.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "devDependencies": {
    "eslint": "^7.21.0",
    "prettier": "^2.2.1"
  }
}
